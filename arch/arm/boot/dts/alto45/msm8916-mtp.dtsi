                       pinctrl-0 = <&nfc_pn547_ven &nfc_pn547_int &nfc_pn547_firm >;
/* [PLATFORM]-Mod-END by TCTNB.XQJ */
                       clocks = <&clock_rpm clk_bb_clk2>;
                       clock-names = "ref_clk";
               };
        };
