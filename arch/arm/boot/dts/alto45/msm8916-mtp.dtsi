               nfc@28 {
                       compatible = "nxp,pn547";
                       reg = <0x28>;
                       nxp,irq-gpio = <&msm_gpio 21 0x00>;
                       nxp,ven-gpio = <&msm_gpio 20 0x00>;
                       nxp,firm-gpio = <&msm_gpio 16 0x00>;
                       nxp,clk-src = "BBCLK2";
                       nxp,clk-en-gpio = <&msm_gpio 0 0x00>;
                       interrupt-parent = <&msm_gpio>;
                       interrupts = <21 0>;
                       qcom,clk-gpio = <&pm8916_gpios 2 0>;
